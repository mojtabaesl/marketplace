{
  "name": "@marketplace/ui",
  "version": "0.0.1",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    "./*": {
      "import": "./src/*",
      "default": "./src/*"
    },
    "./icons": {
      "import": "./src/icons/index.ts",
      "default": "./src/icons/index.ts"
    },
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "dependencies": {
    "@radix-ui/react-slot": "1.2.3",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.540.0",
    "tailwind-merge": "3.3.1"
  }
}
